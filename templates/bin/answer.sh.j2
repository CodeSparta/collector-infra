################################################################################
# AWS Region & Target Env:
#  Target Options:
#   - fences
#   - govcloud
export k9AwsRegion="us-gov-west-1"
export k9TargetEnvironment="govcloud"

################################################################################
# AWS Access Key ID & Secret
export k9AwsKeyId="XXXXXX"
export k9AwsKeySecret="XXXXXXXXX"

################################################################################
# VPC Name
export k9NameVpc="iamgroot"

# Cluster TLD
export k9NameDomain="i.am.groot"

# Cluster Subdomain
export k9NameCluster="cluster"

# Cluster Base FQDN
export k9ClusterDomain="cluster.i.am.groot"

# AWS Red Hat CoreOS AMI ID
export k9RhcosAmi="ami-XXXXXXXXX"

# AWS VPC ID
export k9VpcId="vpc-XXXXXXXXX"

################################################################################
# Private VPC Subnet IDs
export subnet_list='["subnet-XXXXXXXXX", "subnet-XXXXXXXXX", "subnet-XXXXXXXXX"]'
export k9SubnetList='["subnet-XXXXXXXXX", "subnet-XXXXXXXXX", "subnet-XXXXXXXXX"]'
export k9VpcCidr="10.0.0.0/16"

# General Settings
export versOCP="4.5.5"
export varVerOpenshift="4.5.5"
export k9DirCluster="/root/deploy/cluster"
